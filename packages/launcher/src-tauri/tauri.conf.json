{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "open-wow-launcher",
  "version": "0.1.0",
  "identifier": "com.open-wow-launcher.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "transparent": true,
        "decorations": false,
        "fullscreen": false,
        "resizable": false,
        "title": "open-wow-launcher",
        "width": 1352,
        "height": 668,
        "maxWidth": 1352,
        "maxHeight": 668,
        "minWidth": 1352,
        "minHeight": 668,
        "center": true,
        "maximizable": false,
        "maximized": false,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
